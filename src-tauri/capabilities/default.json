{
  "identifier": "default",
  "description": "Default capabilities for Fluent Diary",
  "windows": ["main", "auth-window"],
  "permissions": [
    "core:default",
    "core:window:allow-create",
    "core:webview:allow-create-webview-window",
    "shell:allow-open",
    "core:webview:allow-internal-toggle-devtools",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install"
  ]
}

